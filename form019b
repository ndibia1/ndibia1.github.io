    function yesnoCheck() {
        if (document.getElementById('ContentPlaceHolderDefault_SendMails_1_rdNBDcustomer_0').checked) {
            document.getElementById('ContentPlaceHolderDefault_SendMails_1_txtAccountNo').style.display = 'inline';
            //document.getElementById('ContentPlaceHolderDefault_SendMails_1_RequiredFieldValidator7').style.display = 'inline';
            document.getElementById('trAccountNo').style.display = 'inline';
        }
        else if (document.getElementById('ContentPlaceHolderDefault_SendMails_1_rdNBDcustomer_1').checked) {

            document.getElementById('ContentPlaceHolderDefault_SendMails_1_txtAccountNo').style.display = 'none';
            //document.getElementById('ContentPlaceHolderDefault_SendMails_1_RequiredFieldValidator7').style.display = 'none';
            document.getElementById('trAccountNo').style.display = 'none';
            //ContentPlaceHolderDefault_SendMail_Ar_2_
            //ContentPlaceHolderDefault_SendMail_2_
        }
    }

